{
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "myQueueItem",
      "direction": "in",
      "path": "change-me-to-your-iot-hub-name",
      "connection": "iothub-connection",
      "consumerGroup": "$Default"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "iot-messages/{rand-guid}.csv",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "outputTable",
      "tableName": "iottable",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}